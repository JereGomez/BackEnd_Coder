  {{#if productos}}
    <table class="ui celled table">
    <thead>
      <tr><th>Nombre</th>
      <th>Precio</th>
      <th>Imagen</th>
    </tr></thead>
    <tbody>
       {{#each productos }}
          <tr>
            <td >{{this.nombre}}</td>
            <td >{{this.precio}}</td>
            <td><img src={{this.foto}} style="width: 50px; , height: 50px"></td>
          </tr>
        {{/each}}
    </tbody>
  </table>
    {{else}}
        <h3 class="ui header">No se encontraron productos</h3>
  {{/if}}



